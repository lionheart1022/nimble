.h-skill-bar {
  margin-top: 0;
  @include font-size(1.2);
  font-weight: 700;
  letter-spacing: 1px;
  line-height: 1;
  text-transform: uppercase;
}

.x-skill-bar {
  height: 28px;
  margin-bottom: $baseMargin;
  background-color: $bodyBackground;
}

.x-skill-bar .bar {
  overflow: hidden;
  position: relative;
  width: 0%;
  height: 100%;
  float: left;
  background-color: $accentColor;
}

.x-skill-bar .bar .percent {
  position: absolute;
  right: 3px;
  bottom: 6px;
  padding: 0 5px;
  @include font-size(1.4);
  line-height: 17px;
  color: $baseModBackground;
}



